{
  "name": "tree-sitter-groovy",
  "version": "1.0.0",
  "description": "Groovy grammar for tree-sitter",
  "main": "bindings/node",
  "scripts": {
    "test_class": "tree-sitter parse examples/TestClass > test_class",
    "test_enum": "tree-sitter parse examples/TestEnum > test_enum",
    "test_interface": "tree-sitter parse examples/TestInterface > test_interface",
    "gen": "tree-sitter generate && npm run test_class && npm run test_enum && npm run test_interface"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.7"
  }
}

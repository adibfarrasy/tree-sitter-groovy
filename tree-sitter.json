{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "groovy",
      "camelcase": "groovy",
      "title": "Groovy",
      "scope": "source.groovy",
      "file-types": [
        "groovy", "gvy", "gy", "gsh"
      ],
      "injection-regex": "^groovy$",
      "class-name": "groovy"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "groovy grammar for tree-sitter",
    "authors": [
      {
        "name": "Adib Farrasy",
        "email": "adibfarrasy@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-groovy"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
